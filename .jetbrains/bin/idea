#!/bin/bash
#Generated by JetBrains Toolbox 2.0.4.17212 at 2023-10-17T07:36:26.446459

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "Intellij IDEA" $wait --args "${intellij_args[@]}"
